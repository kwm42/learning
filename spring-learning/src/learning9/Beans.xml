<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="helloWorld" class="learning9.HelloWorld">
		<property name="message" value="Hello World!" />
	</bean>

	<bean id="cStartEventHandler" class="learning9.CStartEventHandler" />

	<bean id="cStopEventHandler" class="learning9.CStopEventHandler" />

	<!-- custom event below -->
	<bean id="customEventHandler" class="learning9.CustomEventHandler" />

	<bean id="customEventPublisher"
		class="learning9.CustomEventPublisher" />

</beans>